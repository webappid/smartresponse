{
  "name": "webappid/smartresponse",
  "version": "3.0.2",
  "description": "SmartResponse simplifies consistent response handling in Laravel: supports Blade, redirect with messages, and JSON format.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^12.0",
    "illuminate/database": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "WebAppId\\SmartResponse\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
