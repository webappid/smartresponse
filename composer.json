{
  "name": "webappid/smartresponse",
  "description": "Smart Response to Blade / Redirect with message / and json format",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\SmartResponse\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\SmartResponse\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "7.2.6",
    "mockery/mockery": "dev-master@dev",
    "orchestra/testbench": "~3.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "WebAppId\\SmartResponse\\ServiceProvider"
      ]
    }
  }
}
